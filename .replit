args = "BTC Analysis Service"

[[workflows.workflow]]
name = "BTC Analysis Service"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main_service.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
